<div class="paginated_collection_contents">
<div class="index_content">
<div class="index_as_table">
<table border="0" cellspacing="0" cellpadding="0" id="index_table_admin_users" class="index_table index" paginator="true">
<thead>
<tr>
  <th class="sortable sorted-desc col col-id"><a href="/admin/caregivers?order=id_asc">Id</a></th>
  <th class="sortable col col-email"><a href="/admin/caregivers?order=email_desc">Email</a></th>
  <th class="sortable col col-current_sign_in_at"><a href="/admin/caregivers?order=current_sign_in_at_desc">First Name</a></th>
  <th class="sortable col col-sign_in_count"><a href="/admin/caregivers?order=sign_in_count_desc">Last Name</a></th>
  <th class="sortable col col-created_at">Avatar</th>
  <th class="col col-actions"></th>
</tr>
</thead>
<tbody>
	<%@users.each do |user|%>
<tr class="odd" id="admin_caregiver_<%=user.id%>">
  <td class="col col-id"><a class="resource_id_link" href="/admin/admin_caregiver/<%=user.id%>"><%=user.id%></a></td>
  <td class="col col-email"><%=user.email%></td>
  <td class="col col-current_sign_in_at"><%= user.first_name %></td>
  <td class="col col-sign_in_count"><%= user.first_name %></td>
  <td class="col col-created_at"><%=image_tag user.avatar.url ,width: '30px;' %></td>
  <td class="col col-actions">
    <div class="table_actions">
<a class="view_link member_link" title="View" href="/admin/caregivers/<%=user.id%>">View</a><a class="edit_link member_link" title="Edit" href="/admin/caregivers/<%=user.id%>/edit">Edit</a><a class="delete_link member_link" title="Delete" data-confirm="Are you sure you want to delete this?" rel="nofollow" data-method="delete" href="/admin/caregivers/<%=user.id%>">Delete</a></div>
  </td>
</tr>
<%end%>
</div>

</tbody>
</table>
</div>
</div>
</div>